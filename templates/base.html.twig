<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
        {% block stylesheets %}
        {% endblock %}

        {% block javascripts %}
            <script type="application/javascript" src={{ asset("library/jquery/jquery.js") }}></script>
            <script type="text/javascript" src={{ asset("library/plupload/plupload.full.min.js") }}></script>
            {% block importmap %}
                {{ importmap('app') | raw | replace({'<script': '<script nonce="' ~ csp_nonce('script') ~ '"' }) | raw }}
            {% endblock %}
        {% endblock %}
    </head>
    <body>
    <div class="container">
        {% block body %}{% endblock %}
    </div>
    </body>
</html>
